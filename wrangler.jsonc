{
  "name": "anime-sveltekit",
  "type": "javascript",
  "main": ".svelte-kit/cloudflare/index.js",
  "site": {
    "bucket": ".svelte-kit/cloudflare/public"
  },
  "build": {
    "command": "npm run build",
    "cwd": "./",
    "watch_paths": ["src/**/*.{js,ts,svelte}"]
  },
  "env": {
    "production": {
      "name": "anime-sveltekit-prod"
    }
  },
  "compatibility_date": "2025-12-02"
}
